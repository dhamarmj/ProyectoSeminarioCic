                     Orientation="Vertical" VerticalOptions="Center">
        <Label Text="Pagina de Inicio" />
        <Label Text="Bienvenido!" Font="30" HorizontalOptions="StartAndExpand"/>
        <DatePicker x:Name="fechaview"/>
        <Button x:Name="btnfecha" Text="fecha"/>
        <TimePicker/>
    </StackLayout>

 </ContentPage>